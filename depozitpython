per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
money = int(input("Введите сумму которую хотите инвестировать:"))
tkb = per_cent['ТКБ']
ckb = per_cent['СКБ']
vtb = per_cent['ВТБ']
sber = per_cent['СБЕР']
depozit = [int(tkb * money / 100), int(ckb * money / 100), int(vtb * money / 100), int(sber * money / 100)]
print(depozit)

max_number = max(depozit)
print("Максимальная сумма,которую вы можете заработать - ", max_number)
